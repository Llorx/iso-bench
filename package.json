{
  "name": "iso-bench",
  "version": "1.0.3",
  "description": "Small benchmark library focused on avoiding optimization/deoptimization pollution between tests by isolating them.",
  "types": "./types/index.d.ts",
  "browser": "./lib/iso-bench.browser.js",
  "main": "./lib/iso-bench",
  "exports": {
    "import": "./lib/iso-bench.mjs",
    "require": "./lib/iso-bench.js"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "keywords": [
    "benchmark",
    "performance",
    "workers",
    "context",
    "pollution",
    "opt",
    "deopt",
    "optimizations",
    "deoptimizations"
  ],
  "scripts": {
    "watch": "npx tsc --watch --inlineSourcemap",
    "build": "npx rollup -c"
  },
  "author": "Llorx",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/llorx/iso-bench"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^18.6.3",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.4.4"
  }
}
